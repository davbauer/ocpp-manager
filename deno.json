{
  "tasks": {
    "dev": "deno run --allow-all --watch ./src/main.ts",
    "migrate:run": "deno run --allow-write --allow-read --allow-sys --allow-env ./src/db/run.ts",
    "migrate:new": "deno run --allow-write --allow-read ./src/db/new.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "kysely-codegen": "npm:kysely-codegen@^0.17.0",
    "react": "npm:react"
  },
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment"
  }
}
